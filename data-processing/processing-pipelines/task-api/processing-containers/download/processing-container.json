{
    "name": "data-download-processing-container",
    "description": "Processing container for downloading data from a URL",
    "templates": [
        {
            "identifier": "dicom-download",
            "description": "Download all identifiers from a kaapana-backend dataset",
            "env": [
                {
                    "name": "DATASET",
                    "value": "",
                    "choices": [],
                    "description": "Name of the dataset"
                },
                {
                    "name": "PARALLEL_DOWNLOADS",
                    "value": "3",
                    "choices": [],
                    "description": "Number of threads used for downloading the data"
                },
                {
                    "name": "KAAPANA_PROJECT_IDENTIFIER",
                    "value": "",
                    "choices": [],
                    "description": "Opensearch index to get metadata information"
                },
                {
                    "name": "KAAPANA_PROJECT_USER_NAME",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_PROJECT_USER_PASSWORD",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_OPENSEARCH_URL",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_OPENSEARCH_HOST",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_OPENSEARCH_PORT",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_KEYCLOAK_URL",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_CLIENT_SECRET",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_CLIENT_ID",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_DICOM_WEB_FILTER_URL",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_BACKEND_URL",
                    "value": "",
                    "choices": [],
                    "description": ""
                },
                {
                    "name": "KAAPANA_NOTIFICATION_URL",
                    "value": "",
                    "choices": [],
                    "description": ""
                }

                
            ],
            "inputs": [],
            "outputs": [
                {
                    "name": "downloads",
                    "mounted_path": "/home/kaapana/downloads"
                }
            ],
            "command": [
                "python3",
                "-u",
                "download_dicoms.py"
            ],
            "resources": {
                "limits": {
                    "memory": "500Mi"
                },
                "requests": {
                    "memory": "100Mi"
                }
            }
        }
    ]
}